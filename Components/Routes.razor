@namespace JournalApp.Components
@using JournalApp.Shared

<Router AppAssembly="@typeof(Routes).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(JournalApp.Shared.MainLayout)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(JournalApp.Shared.MainLayout)">
            <p>Page not found.</p>
        </LayoutView>
    </NotFound>
</Router>
